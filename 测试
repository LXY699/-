-- 汉化脚本
-- 江砚辰破解北海

local Translations = {["KronHub"] = "克朗中心",
    ["Language"] = "语言",
    ["Theme"] = "主题",
    ["Home"] = "主页",
    ["Player"] = "玩家",
    ["World"] = "世界",
    ["Teleport"] = "传送",
    ["Auto Buy"] = "自动购买",
    ["Slot"] = "插槽",
    ["Wood"] = "木材",
    ["Dupe"] = "复制",
    ["Open/Close"] = "打开/关闭",
    }

local function translateText(text)
    if not text or type(text) ~= "string" then return text end
    
    if Translations[text] then
        return Translations[text]
